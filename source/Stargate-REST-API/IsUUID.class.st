Class {
	#name : #IsUUID,
	#superclass : #IsObject,
	#category : #'Stargate-REST-API'
}

{ #category : #'type constraint' }
IsUUID class >> matchesTo: aString [

	^ aString isString
		and: [ '[0-9a-f]{8}\-[0-9a-f]{4}\-[0-9a-f]{4}\-[0-9a-f]{4}\-[0-9a-f]{12}' asRegex
				matches: aString ]
]

{ #category : #'type constraint' }
IsUUID class >> parseString: aString [

	^ UUID fromString: aString
]
